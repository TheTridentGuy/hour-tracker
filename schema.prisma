datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
generator client {
  provider             = "prisma-client-py"
  interface            = "sync"
  recursive_type_depth = 5
}

model User {
  slack_id String @id
  signed_in Boolean @default(false)
  signin_time DateTime?
  total_hours Float @default(0)
}